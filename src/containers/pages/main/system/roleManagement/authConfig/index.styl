#authConfig
  padding 30px 40px
  .headerTitle
    color rgba(51,51,51,1)
    font-size 16px
    line-height 22px
    font-weight 600
  .title
    background rgba(245,245,245,1)
    border 0.005rem solid rgba(232,232,232,1)
    text-align center
    color rgba(51,51,51,1)
    font-size 16px
    line-height 50px
    margin 20px 0 10px 0
    font-weight 600
  .tableDiv
    border 0.005rem solid rgba(232,232,232,1)
    margin-bottom 10px
    .radioRow
      font-weight 600
    .tableTitleRow
      background-color #F2F6FC
      height 50px
      padding-left 20px
      display flex
      font-weight 600
      align-items center
      border-bottom 0.005rem solid rgba(232,232,232,1)
      .ant-form-item
        margin-bottom 0 !important
    .tabsDiv
      padding 20px 40px 20px 40px
      .tabPaneDiv
        padding-left 16px
    .childrenRow
      display inline-block
      margin-right 10px
    .ant-tabs.ant-tabs-card .ant-tabs-card-bar .ant-tabs-tab-active
      background #B2D2ED
      color #333333
      border-bottom none
    .ant-tabs.ant-tabs-card .ant-tabs-card-bar .ant-tabs-tab
      margin-right 10px
      border-radius 6px 6px 0 0
  .authBtn
    margin-top 40px
    button
      width 160px
      margin-right 30px
  .contract-com-margin-r
    .ant-select-selection__rendered
      display flex
      align-items center
  .ant-form-item
    margin-bottom 0
