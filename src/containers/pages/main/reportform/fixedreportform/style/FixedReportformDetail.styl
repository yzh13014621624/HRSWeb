 #fixed-reportform-detail {
  padding: 30px 0 20px 20px;
  .fixed-reportform-title {
    font-size:16px;/*no*/
    font-weight:500;
    color:rgba(51,51,51,1);
    line-height:22px;
    padding-bottom: 15px;
  }
  .fixed-reportform-sohu {
    margin-bottom: 20px;
  }
  .btn-style {
    width:160px;
    height:38px;
    background:rgba(64,169,255,1);
    border-radius:5px;
    font-size:14px;
    font-weight:500;
    color:rgba(255,255,255,1);
    line-height:20px;
    margin-right: 30px;
  }
  .btn-margin {
    margin-left: 30px;
  }
  .return_button {
    width:160px;
    height:38px;
    background:rgba(255,255,255,1);
    border-radius:5px;
    border:1px solid rgba(64,169,255,1);/*no*/
    font-size:14px;
    font-weight:500;
    color:rgba(64,169,255,1);
    line-height:20px;
    // margin-left: 30px;
  }
  .fixed-reportform-btn-mt {
    margin-top: 30px;
  }
}
