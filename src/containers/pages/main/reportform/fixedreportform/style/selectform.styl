#select-form-box
  span,p
    font-size 14px
  .ant-form-item
    width 100%
    .ant-form-item-children
      width 100%
      .ant-radio-group
        width 100%
        text-align left
    .select-item
      display block
      width 100%
      padding 5px 15px
      border 1px solid #F0F0F0;/*no*/
      border-radius 5px
      margin-bottom 13px
.sumbit-btn
  font-size 14px
  font-weight 500
  color rgba(255,255,255,1)
  line-height 20px
  margin-right 30px
  width 160px
  height 38px
  border none

.cancel-btn
  border-radius 5px
  width 160px
  height 38px
  font-size 14px
  font-weight 500
  color rgba(64,169,255,1)
  border 1px solid rgba(64,169,255,1);/*no*/