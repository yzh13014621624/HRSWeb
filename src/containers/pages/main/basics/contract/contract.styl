 #contract-page {
   padding: 0 20px 20px 20px;
   .contract-page-button {
     margin-right: 10px;
   }
   .contract-test-font {
     font-size: 14px;/*no*/
     line-height: 20px;
     padding-right: 7px;
     font-weight:400;
     color:rgba(51,51,51,1);
   }
 }

//  公共样式
 .contract-common {
   width: 220px;
   height: 32px;
 }
 .contract-com-margin-r {
   margin-right: 20px
 }
 .contract-com-margin-r-10 {
   margin-right: 10px;
 }
 .data-width {
   width: 300px;
 }
 .row-margin {
  margin-top: 10px;
  margin-bottom: 20px;
 }


//  试用期详情
.contract-try-modal {
  .ant-modal-body {
    padding: 25px 20px 30px 20px;
  }
  .sumbit-btn {
    font-size: 14px;
    font-weight: 500;
    color: rgba(255,255,255,1);
    line-height: 20px;
    width: 160px;
    height: 38px;
    margin: 40px 30px 5px 0;
  }
  .cancel-btn {
    border-radius: 5px;
    width: 160px;
    height: 38px;
    font-size: 14px;
    font-weight: 500;
    color: rgb(40, 145, 231);
    border:1px solid rgba(64,169,255,1);/*no*/
   }
  .cfrom-item {
    width: 2.04rem;
    font-size:14px;
    margin-right: 0;
    font-weight:400;
    color:rgba(102,102,102,1);
    padding-bottom: 5px;
    >span {
      width: 220px;
    }
    .ant-form-item-control {
      line-height: 39px;/*no*/
    }
  }

}
